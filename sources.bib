file = {:C$\backslash$:/Users/dst/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Leemans, Fahland, van der Aalst - 2013 - Discovering Block-Structured Process Models from Event Logs - A Constructive Approach(2).pdf:pdf},
isbn = {9783642386961},
issn = {03029743},
keywords = {block-structured process models,fitness,process discovery,soundness},
mendeley-groups = {Workflow Mining/Inductive Miner},
pages = {311--329},
publisher = {Springer, Berlin, Heidelberg},
title = {{Discovering block-structured process models from event logs - A constructive approach}},
url = {http://link.springer.com/10.1007/978-3-642-38697-8{\_}17},
volume = {7927 LNCS},
year = {2013}
}

@inproceedings{InductiveVisualMiner2014,
abstract = {Process mining aims to extract information from recorded process data, which can be used to gain insights into the process. This requires applying a discovery algorithm and settings its parameters, after which the discovered process model should be evaluated. Both steps may need to be repeated several times until a satisfying model is found; we refer to this as process exploration. Existing commercial tools usually do not provide models having executable semantics, thereby disallowing for accurate map evaluation, while most academic tools lack features and by the repetitive nature of process exploration, their use is tedious. In this paper, we describe a novel process exploration tool: the Inductive visual Miner. It aims to bridge this gap between commercial and academic tools, by combining the executable semantics of academic tools with the exploration support of commercial tools. It also adds animation and deviation visualisation capabilities. Copyright {\textcopyright} 2014 for this paper by its authors. Copying permitted for private and academic purposes.},
author = {Leemans, Sander J.J. and Fahland, Dirk and {Van Der Aalst}, Wil M.P.},
booktitle = {CEUR Workshop Proceedings},
file = {:C$\backslash$:/Users/dst/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Leemans, Fahland, Van Der Aalst - Unknown - Process and Deviation Exploration with Inductive visual Miner.pdf:pdf},
issn = {16130073},
keywords = {Deviation analysis,Process exploration,Process mining},
mendeley-groups = {Workflow Mining/Inductive Miner},
pages = {46--50},
title = {{Process and deviation exploration with inductive visual miner}},
url = {http://ceur-ws.org/Vol-1295/paper19.pdf},
volume = {1295},
year = {2014}
}

