isbn = {WP 166},
issn = {03029743},
journal = {Technische Universiteit Eindhoven, Tech. Rep. WP},
keywords = {busi,knowledge discovering,ness process intelligence,process mining,process mining benchmark},
mendeley-groups = {Workflow Mining},
pages = {1--34},
pmid = {21258659},
title = {{Process Mining with the Heuristics Miner Algorithm}},
url = {https://pdfs.semanticscholar.org/1cc3/d62e27365b8d7ed6ce93b41c193d0559d086.pdf http://cms.ieis.tue.nl/Beta/Files/WorkingPapers/Beta{\_}wp166.pdf},
